{
  "name": "blockfinder",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "start": "node build/src/index.js",
    "start:test:e2e": "dotenv -e test/.env.test yarn dev",
    "test": "PORT=3034 start-server-and-test --expect 405 'yarn start:test:e2e' 3034 'dotenv -e test/.env.test jest --runInBand'"
  },
  "eslintConfig": {
    "extends": "@snapshot-labs"
  },
  "prettier": "@snapshot-labs/prettier-config",
  "dependencies": {
    "@graphql-tools/schema": "^8.3.10",
    "@snapshot-labs/snapshot-metrics": "^1.4.1",
    "@snapshot-labs/snapshot-sentry": "^1.5.5",
    "@snapshot-labs/snapshot.js": "^0.11.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^16.6.0",
    "graphql-fields": "^2.0.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@snapshot-labs/eslint-config": "^0.1.0-beta.15",
    "@snapshot-labs/prettier-config": "^0.1.0-beta.15",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.8.5",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.28.0",
    "jest": "^29.6.2",
    "jest-environment-node-single-context": "^29.1.0",
    "prettier": "^3.0.3",
    "start-server-and-test": "^2.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": "^18.0.0"
  }
}
